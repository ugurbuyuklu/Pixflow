{
  "name": "pixflow",
  "version": "1.0.0",
  "description": "Multi-product AI asset generation platform for Pixery",
  "private": true,
  "scripts": {
    "dev": "npm run dev:web",
    "dev:web": "bash ./scripts/dev-web.sh",
    "dev:web:server": "PORT=${PIXFLOW_WEB_API_PORT:-3002} tsx watch src/server/index.ts",
    "dev:web:client": "vite --config vite.web.config.ts",
    "dev:server": "tsx watch src/server/index.ts",
    "build": "npm run build:web",
    "build:web": "vite build --config vite.web.config.ts",
    "preview": "npm run preview:web",
    "preview:web": "vite preview --config vite.web.config.ts",
    "start:web": "PORT=${PIXFLOW_WEB_API_PORT:-3002} tsx src/server/index.ts",
    "cf:whoami": "wrangler whoami",
    "deploy:pages": "bash ./scripts/deploy-pages.sh production",
    "deploy:pages:preview": "bash ./scripts/deploy-pages.sh preview",
    "dev:worker:api": "BACKEND_ORIGIN=${BACKEND_ORIGIN:-http://localhost:${PIXFLOW_WEB_API_PORT:-3002}} wrangler dev --config wrangler.api-proxy.toml",
    "deploy:worker:api": "bash ./scripts/deploy-worker-api.sh",
    "dev:container": "wrangler dev --config wrangler.container.toml",
    "deploy:container": "bash ./scripts/deploy-container.sh",
    "container:list": "wrangler containers list",
    "container:images": "wrangler containers images list",
    "pgp:lock:check": "node scripts/pgp-lock-guard.js check",
    "pgp:lock:update": "node scripts/pgp-lock-guard.js update",
    "native:rebuild": "npm run native:rebuild:node",
    "native:rebuild:node": "npm rebuild better-sqlite3",
    "lint": "tsc --noEmit",
    "lint:biome": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "pretest": "npm run native:rebuild:node",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "smoke:api": "tsx src/server/smoke/criticalPath.ts",
    "smoke:journey": "tsx src/server/smoke/journeyCriticalPaths.ts",
    "smoke:external": "tsx src/server/smoke/externalPipeline.ts",
    "smoke:external:real": "tsx src/server/smoke/externalPipeline.ts --real",
    "telemetry:report": "tsx src/server/telemetry/report.ts",
    "telemetry:report:json": "tsx src/server/telemetry/report.ts --json",
    "telemetry:trends": "tsx src/server/telemetry/trends.ts",
    "telemetry:dashboard": "node scripts/build-telemetry-dashboard.js",
    "telemetry:highlights": "node scripts/build-telemetry-highlights.js",
    "telemetry:baseline": "node scripts/update-telemetry-baseline.js",
    "preflight:release": "node scripts/build-release-preflight.js --profile release",
    "preflight:nightly": "node scripts/build-release-preflight.js --profile nightly",
    "telemetry:check": "tsx src/server/telemetry/checkThresholds.ts --profile ci",
    "telemetry:check:ci": "tsx src/server/telemetry/checkThresholds.ts --profile ci",
    "telemetry:check:nightly": "tsx src/server/telemetry/checkThresholds.ts --profile nightly",
    "telemetry:check:release": "tsx src/server/telemetry/checkThresholds.ts --profile release",
    "telemetry:check:frontend": "tsx src/server/telemetry/checkFrontendPerf.ts",
    "telemetry:check:frontend:ci": "tsx src/server/telemetry/checkFrontendPerf.ts --profile ci",
    "telemetry:check:frontend:nightly": "tsx src/server/telemetry/checkFrontendPerf.ts --profile nightly",
    "telemetry:check:frontend:release": "tsx src/server/telemetry/checkFrontendPerf.ts --profile release",
    "telemetry:check:regression": "tsx src/server/telemetry/checkRegression.ts",
    "telemetry:check:regression:warn": "tsx src/server/telemetry/checkRegression.ts --mode warn",
    "telemetry:check:regression:block": "tsx src/server/telemetry/checkRegression.ts --mode block",
    "validate:playbooks": "node scripts/validate-playbook-registry.js",
    "threshold:propose": "node scripts/propose-threshold-update.js",
    "preflight:history": "node scripts/build-preflight-history.js",
    "alert:dedup": "node scripts/build-nightly-alert-dedup.js",
    "gate:release": "bash ./scripts/gate-release.sh"
  },
  "dependencies": {
    "@distube/yt-dlp": "^2.0.1",
    "@fal-ai/client": "^1.8.5",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "cheerio": "^1.2.0",
    "cors": "^2.8.5",
    "diff-match-patch": "^1.0.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "ffmpeg-static": "^5.3.0",
    "jsonwebtoken": "^9.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.469.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.17.0",
    "puppeteer": "^24.37.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-hot-toast": "^2.6.0",
    "uuid": "^9.0.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@cloudflare/containers": "^0.1.0",
    "@biomejs/biome": "^2.3.14",
    "@tailwindcss/vite": "^4.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/diff-match-patch": "^1.0.36",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20 <21"
  },
  "author": "Pixery",
  "license": "UNLICENSED"
}
